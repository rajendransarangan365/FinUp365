{
    "version": 2,
    "functions": {
        "api/index.js": {
            "runtime": "nodejs20.x",
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}